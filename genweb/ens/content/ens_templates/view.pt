<html xmlns="http://www.w3.org/1999/xhtml"
            xmlns:metal="http://xml.zope.org/namespaces/metal"
            xmlns:tal="http://xml.zope.org/namespaces/tal"
            xmlns:i18n="http://xml.zope.org/namespaces/i18n"
            metal:use-macro="context/main_template/macros/master">

<body>
  <metal:custom_title fill-slot="content-title">
    <h1 class="documentFirstHeading" tal:content="context/title"></h1>
  </metal:custom_title>
  <metal:content-core fill-slot="content-core">
    <section id="ens-metadata" class="ens-metadata">
        <h2 i18n:translate="Dades indentificatives" class="collapsable">
            Dades identificatives
        </h2>
        <div>
        <dl>
          <dt i18n:translate="Denominació">Denominació</dt>
          <dd tal:content="context/title"/>

          <dt i18n:translate="Descripció">Descripció</dt>
          <dd tal:content="python: context.description or '-'"/>

          <dt i18n:translate="Acrònim">Acrònim</dt>
          <dd tal:content="python: context.acronim or '-'"/>

          <dt i18n:translate="Codi de classificació">Codi de classificació</dt>
          <dd tal:content="python: context.codi or '-'"/>

          <dt i18n:translate="NIF">NIF</dt>
          <dd tal:content="python: context.nif or '-'"/>

          <dt i18n:translate="Figura jurídica">Figura jurídica</dt>
          <dd tal:content="python: context.figura_juridica or '-'"/>

          <dt i18n:translate="Núm. Identif.">Núm. Identif.</dt>
          <dd tal:content="python: context.numero_identificacio or '-'"/>

          <dt i18n:translate="Estat">Estat</dt>
          <dd tal:content="python: context.estat or '-'"/>

          <dt i18n:translate="Domicili social">Domicili social</dt>
          <dd tal:content="python: context.domicili_social or '-'"/>

          <dt i18n:translate="Adreça oficines">Adreça oficines</dt>
          <dd tal:content="python: context.adreca_oficines or '-'"/>

          <dt i18n:translate="Web">Web</dt>
          <dd tal:content="python: context.web or '-'"/>

          <dt i18n:translate="Tipologia">Tipologia</dt>
          <dd tal:content="python: context.tipologia or '-'"/>
        </dl>
        <h3 i18n:translate="Càrrecs directius">Càrrecs directius</h3>
        <tal:block define="directius view/get_directius_obj">
        <table tal:condition="directius">
          <thead>
            <tr>
              <th i18n:translate="Nom" class="column-medium">Nom</th>
              <th i18n:translate="Càrrec" class="column-medium">Càrrec</th>
              <th i18n:translate="Telèfon" class="column-small">Telèfon</th>
              <th i18n:translate="Email">Email</th>
            </tr>
          </thead>
          <tbody>
            <tr tal:repeat="directiu directius">
              <td>
                <a tal:attributes="href directiu/absolute_url"
                   tal:content="directiu/title"/>
              </td>
              <td tal:content="python: directiu.carrec or '-'"/>
              <td tal:content="python: directiu.telefon or '-'"/>
              <td tal:content="python: directiu.email or '-'"/>
            </tr>
          </tbody>
        </table>
        <p tal:condition="not: directius"
          i18n:translate="No hi ha càrrecs directius">No hi ha càrrecs directius</p>
        </tal:block>
        <h3 i18n:translate="Persones de contacte">Persones de contacte</h3>
        <tal:block define="contactes view/get_contactes_obj">
        <table tal:condition="contactes">
          <thead>
            <tr>
              <th i18n:translate="Nom" class="column-medium">Nom</th>
              <th i18n:translate="Càrrec" class="column-medium">Càrrec</th>
              <th i18n:translate="Telèfon" class="column-small">Telèfon</th>
              <th i18n:translate="Email">Email</th>
            </tr>
          </thead>
          <tbody>
            <tr tal:repeat="contacte contactes">
              <td>
                <a tal:attributes="href contacte/absolute_url"
                   tal:content="contacte/title"/>
              </td>
              <td tal:content="python: contacte.carrec or '-'"/>
              <td tal:content="python: contacte.telefon or '-'"/>
              <td tal:content="python: contacte.email or '-'"/>
            </tr>
          </tbody>
        </table>
        <p tal:condition="not: contactes"
          i18n:translate="No hi ha persones de contacte">No hi ha persones de contacte</p>
        </tal:block>
        </div>

        <h2 i18n:translate="Informació de participació">
            Informació de participació
        </h2>
        <div>
        <dl>
          <dt i18n:translate="Aportació inicial">Aportació inicial</dt>
          <dd tal:content="view/get_aportacio"/>

          <dt i18n:translate="Quota">Quota</dt>
          <dd tal:content="view/get_quota"/>

          <dt i18n:translate="Unitat de càrrec">Unitat de càrrec</dt>
          <dd tal:content="python:context.unitat_carrec or '-'"/>

          <dt i18n:translate="Capital social o fons patrimonial">Capital social o fons patrimonial</dt>
          <dd tal:content="view/get_capital_social"/>

          <dt i18n:translate="Observacions">Observacions</dt>
          <dd tal:content="python:context.participacio_observacions or '-'"/>
        </dl>

        <h3 i18n:translate="Percentatges de participació">Percentatges de participació</h3>
        <tal:block define="percentatges view/get_percentatges_participacio_obj">
          <table tal:condition="percentatges">
            <thead>
              <tr>
                <th i18n:translate="Percentatge" class="column-small">Percentatge</th>
                <th i18n:translate="Observacions">Observacions</th>
              </tr>
            </thead>
            <tbody>
              <tr tal:repeat="percentatge percentatges">
                <td><a tal:content="percentatge/Title"
                       tal:attributes="href percentatge/absolute_url"/>
                </td>
                <td tal:content="percentatge/observacions"/>
              </tr>
            </tbody>
          </table>
          <p tal:condition="not: percentatges"
            i18n:translate="No hi ha percentatges de participació">No hi ha percentatges de participació</p>
        </tal:block>

        <h3 i18n:translate="Òrgans de govern">Òrgans de govern</h3>
        <tal:block define="organs_govern view/get_organs_govern">
        <tal:block repeat="organ_govern organs_govern">
          <section class="ens-metadata-level-2">
          <h4><a tal:content="organ_govern/Title"
                 tal:attributes="href organ_govern/getURL"/>
          </h4>
          <tal:block define="carrecs python: view.get_carrecs_by_organ_grouped_by_ens(organ_govern)">
            <tal:block condition="carrecs">
              <input type="checkbox" class="label-aligned selector-input-historic"/>
              <label i18n:translate="Mostra els càrrecs històrics">Mostra els càrrecs històrics</label>
              <table>
              <thead>
                <tr>
                  <th i18n:translate="Ens" class="column-medium">Ens</th>
                  <th i18n:translate="Nom" class="column-medium">Nom</th>
                  <th i18n:translate="Càrrec" class="column-medium">Càrrec</th>
                  <th i18n:translate="Històric" class="column-center">Històric</th>
                </tr>
              </thead>
              <tbody>
                <tal:block repeat="carrecs_by_ens carrecs">
                  <tal:block define="ens python: carrecs_by_ens[0]">
                    <tr tal:repeat="carrec python: carrecs_by_ens[1]"
                        tal:attributes="class python: 'historic' if carrec.is_historic else ''">
                      <td tal:content="ens"/>
                      <td tal:content="carrec/Title"/>
                      <td><a tal:content="carrec/carrec"
                             tal:attributes="href carrec/getURL"/>
                      </td>
                      <td class="column-center">
                        <input type="checkbox" class="selector-td-historic" tal:attributes="checked carrec/is_historic; disabled True"/>
                      </td>
                    </tr>
                  </tal:block>
                </tal:block>
              </tbody>
              </table>
            </tal:block>
            <p tal:condition="not: carrecs"
              i18n:translate="No hi ha càrrecs">No hi ha càrrecs</p>
          </tal:block>
          </section>
        </tal:block>
        <p tal:condition="not: organs_govern"
          i18n:translate="No hi ha òrgans de govern">No hi ha òrgans de govern</p>
        </tal:block>

        <h3 i18n:translate="Òrgans de govern">Òrgans assessors</h3>
        <tal:block define="organs view/get_organs_assessors">
        <tal:block repeat="organ organs">
          <section class="ens-metadata-level-2">
          <h4><a tal:content="organ/Title"
                 tal:attributes="href organ/getURL"/>
          </h4>
          <tal:block define="carrecs python: view.get_carrecs_by_organ_grouped_by_ens(organ)">
            <tal:block condition="carrecs">
              <input type="checkbox" class="label-aligned selector-input-historic"/>
              <label i18n:translate="Mostra els càrrecs històrics">Mostra els càrrecs històrics</label>
              <table>
              <thead>
                <tr>
                  <th i18n:translate="Ens" class="column-medium">Ens</th>
                  <th i18n:translate="Nom" class="column-medium">Nom</th>
                  <th i18n:translate="Càrrec" class="column-medium">Càrrec</th>
                  <th i18n:translate="Històric" class="column-center">Històric</th>
                </tr>
              </thead>
              <tbody>
                <tal:block repeat="carrecs_by_ens carrecs">
                  <tal:block define="ens python: carrecs_by_ens[0]">
                    <tr tal:repeat="carrec python: carrecs_by_ens[1]"
                        tal:attributes="class python: 'historic' if carrec.is_historic else ''">
                      <td tal:content="ens"/>
                      <td tal:content="carrec/Title"/>
                      <td><a tal:content="carrec/carrec"
                             tal:attributes="href carrec/getURL"/>
                      </td>
                      <td class="column-center">
                        <input type="checkbox" class="selector-td-historic" tal:attributes="checked carrec/is_historic; disabled True"/>
                      </td>
                    </tr>
                  </tal:block>
                </tal:block>
              </tbody>
              </table>
            </tal:block>
            <p tal:condition="not: carrecs"
              i18n:translate="No hi ha càrrecs">No hi ha càrrecs</p>
          </tal:block>
          </section>
        </tal:block>
        <p tal:condition="not: organs"
          i18n:translate="No hi ha òrgans de govern">No hi ha òrgans assessors</p>
        </tal:block>

        <h3 i18n:translate="Unitats UPC implicades">Unitats UPC implicades</h3>
        <tal:block define="unitats view/get_unitats_obj">
          <table tal:condition="unitats">
            <thead>
              <tr>
                <th i18n:translate="Unitat" class="column-medium">Unitat</th>
                <th i18n:translate="Persona de referència" class="column-medium">Persona de referència</th>
                <th i18n:translate="Tipus de gestió">Tipus de gestió</th>
              </tr>
            </thead>
            <tbody>
              <tr tal:repeat="unitat unitats">
                <td><a tal:content="unitat/title"
                       tal:attributes="href unitat/absolute_url"/>
                </td>
                <td tal:content="unitat/persona"/>
                <td tal:content="unitat/tipus_gestio"/>
              </tr>
            </tbody>
          </table>
          <p tal:condition="not: unitats"
            i18n:translate="No hi ha unitats UPC implicades">No hi ha unitats UPC implicades</p>
        </tal:block>
        </div>

        <h2 i18n:translate="Marc legal">Marc legal</h2>
        <div>
        <dl>
          <dt i18n:translate="Data de constitució">Data de constitució</dt>
          <dd tal:content="python:context.data_constitucio and context.data_constitucio.strftime('%d/%m/%Y') or '-'"/>

          <dt i18n:translate="Entitats constituents">Entitats constituents</dt>
          <dd tal:content="python:context.entitats_constituents or '-'"/>

          <dt i18n:translate="Entitats actuals">Entitats actuals</dt>
          <dd tal:content="python:context.entitats_actuals or '-'"/>

          <dt i18n:translate="Data d&apos;entrada UPC">Data d&apos;entrada UPC</dt>
          <dd tal:content="python:context.data_entrada and context.data_entrada.strftime('%d/%m/%Y') or '-'"/>

          <dt i18n:translate="Seu social">Seu social</dt>
          <dd tal:content="python:context.seu_social or '-'"/>

          <dt i18n:translate="Observacions">Observacions</dt>
          <dd tal:content="python:context.marc_legal_observacions or '-'"/>
        </dl>

        <h3 i18n:translate="Acords òrgans">Acords òrgans</h3>
        <tal:block define="acords view/get_acords_obj">
        <table tal:condition="acords">
          <thead>
            <tr>
              <th i18n:translate="Data" class="column-small">Data</th>
              <th i18n:translate="Data" class="column-medium">Òrgan</th>
              <th i18n:translate="Descripció" class="column-medium">Descripció</th>
              <th i18n:translate="Fitxer">Fitxer</th>
            </tr>
          </thead>
          <tbody>
            <tr tal:repeat="acord acords">
              <td tal:content="python: acord.data and acord.data.strftime('%d/%m/%Y') or '-'"/>
              <td>
                <a tal:attributes="href python: acord.getParentNode().absolute_url()"
                   tal:content="python: acord.getParentNode().title"/>
              </td>
              <td>
                <a tal:attributes="href acord/absolute_url"
                   tal:content="acord/title"/>
              </td>
              <td>
                <a tal:attributes="href string:${acord/absolute_url}/view/++widget++form.widgets.fitxer/@@download/${acord/fitxer/filename}"
                   tal:content="acord/fitxer/filename"/>
              </td>
            </tr>
          </tbody>
        </table>
        <p tal:condition="not: acords"
          i18n:translate="No hi ha acords">No hi ha acords</p>
        </tal:block>

        <h3 i18n:translate="Escriptures públiques">Escriptures públiques</h3>
        <tal:block define="escriptures view/get_escriptures_obj">
        <table tal:condition="escriptures">
