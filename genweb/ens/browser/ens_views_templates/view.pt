          <dd tal:content="python: context.adreca_oficines_1 or '-'"/>

          <dt i18n:translate="Observacions">Observacions</dt>
          <dd tal:content="python: context.adreca_oficines_1_observacions or '-'"/>

          <dt i18n:translate="Adreça oficines 2">Adreça oficines 2</dt>
          <dd tal:content="python: context.adreca_oficines_2 or '-'"/>

          <dt i18n:translate="Observacions">Observacions</dt>
          <dd tal:content="python: context.adreca_oficines_2_observacions or '-'"/>

          <dt i18n:translate="Telèfon">Telèfon</dt>
          <dd tal:content="python: context.telefon or '-'"/>

          <dt i18n:translate="Web">Web</dt>
          <dd tal:content="python: context.web or '-'"/>

          <dt i18n:translate="Tipologia UPC">Tipologia UPC</dt>
          <dd tal:content="python: context.tipologia or '-'"/>

          <dt i18n:translate="Anotacions">Anotacions</dt>
